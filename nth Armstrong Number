def armstrong(n):
    summ = 0
    temp = n
    order = len(str(n))
    while temp>0:
        d = temp%10
        summ = summ +(d**order)
        temp = temp//10
    print(summ)
    if (summ == n):
        return True
    else:
        return False

n = int(input())
if(armstrong(n)):
    print("true")
else:
    print("false")
